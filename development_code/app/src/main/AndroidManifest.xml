<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartvendingmachine">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartVendingMachine">
        <activity android:name=".cartActivity"></activity>
        <activity android:name=".machine" />
        <activity android:name=".add" />
        <activity android:name=".mainadmin" />
        <activity android:name=".admin" />
        <activity android:name=".cart" />
        <activity android:name=".show" />
        <activity android:name=".Home" />
        <activity android:name=".Login" />
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>